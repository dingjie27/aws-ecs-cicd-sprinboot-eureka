{
    "executionRoleArn": "arn:aws:iam::190547127216:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "eureka",
            "image": "<IMAGE1_NAME>",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "networkMode": "bridge",
    "cpu": "256",
    "memory": "512",
    "family": "ecs-cicd-task"
}
